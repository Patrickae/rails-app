<h1>Articles</h1>
<table>
  <thead>
    <th>Title</th>
    <th>Description</th>
    <th>Action</th>
  </thead>
  <% @articles.each do |article| %>
      <tr>
        <td><%=article.title%></td>
        <td><%=article.description%></td>
        <td>Placeholder</td>
        <td><%= link_to "view", article%></td>
        <td><%= link_to "edit", edit_article_path(article)%></td>
        <td><%= link_to "delete", article_path(article), method: :delete%></td>
      </tr>
  <%end%>

